  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./Scripts/**/**[!favicon.ico]",
    "copy": "npm run copy:css & npm run copy:font",
    "copy:css": "cpx \"./node_modules/element-ui/lib/theme-default/index.css\" \"./Scripts/css\"",
    "copy:font": "cpx \"./node_modules/element-ui/lib/theme-default/fonts/*.*\" \"./Scripts/css/fonts\"",
    "sass": "node-sass ./VueSrc/common.scss -o ./Scripts --output-style compressed && npm run makehtml",
    "rollup": "rollup -c && npm run makehtml",
    "makehtml": "npm run libStat && npm run pug",
    "pug": "pug -O ./listup.json ./ViewPug/Home -o ./Views/Home -P -E cshtml",
    "libStat": "node libStamps.js ./Scripts/*.{js,css} -n test -p"
  },
